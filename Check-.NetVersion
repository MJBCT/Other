#Weryfikacja wersji zainstalowanego .NET-a
Get-ChildItem 'HKLM:\SOFTWARE\Microsoft\NET Framework Setup\NDP' -recurse |Get-ItemProperty -name Version,Release -EA 0 |Where { $_.PSChildName -match '^(?!S)\p{L}'} |Select PSChildName, Version, Release, @{Label=" .Net Version"; Expression={switch($_.Release){"393297"{"4.6"}"394254"{"4.6.1"}"394802"{"4.6.2"}"460805"{"4.7.0"}"461310"{"4.7.1"}"461814"{"4.7.2"}"528049"{"4.8"}}}}
